<script>
$(function() {
    $("#search #submit").click(function(evt){
        evt.preventDefault();
        $(this).parents('form').submit();
    });

	$("#searchTerm").bind('keyup paste', function() {
		doSearch(false);
	});
    /*
    $("#searchTerm").on("focusout", function(){
        $(".search #suggest").delay(400).slideUp(400);
    });*/

	$("#searchBoxSelect").hover(function() {
		$(this).addClass("hover");
	}, function() {
		$(this).removeClass("hover");
	});

    $(".search").hover(function() {

    }, function() {
        $(".search #suggest").delay(400).slideUp(400);
    });
    $('#searchBoxList a').click(function(){
        $('#searchBoxList a').removeClass('selected');
        $(this).addClass('selected');
        $(this).parents('form').find('#searchBoxValue').val($(this).attr('rel'));
        //$('#searchBoxActive').html($(this).html());
        $("#searchBoxSelect").removeClass("hover");
        doSearch(true);

        return false;
    });
});
function doSearch(select) {
    var $form = $('form#search');
    var type = $form.find('#searchBoxValue').val();
    if ($form.find('#searchTerm').val().length > 2 && ((type != 'shop') || select)) {
		$.get($.siteUrl + '/ajax/search', $form.serialize(), function(data) {
			if (data) {
                if(data == 'submit') {
                    $form.submit();
                } else {
                    $(".search #suggest").html(data).slideDown(400);
                }
			} else {
				$(".search #suggest").slideUp(400);
			}
		});
	}
}
