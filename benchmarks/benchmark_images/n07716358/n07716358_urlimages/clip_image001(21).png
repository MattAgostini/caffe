    var isiPad = /ipad/i.test(navigator.userAgent.toLowerCase());
    var isiPhone = /iphone/i.test(navigator.userAgent.toLowerCase());


    function enableTooltip(data) {
    var id = $(data).attr('title');
    var type = ($(data).hasClass('plant')) ? 'plant' : 'user';
    $(data).tooltip({
        effect: 'slide',
        position: 'top center',
        predelay: 500,
        delay: 500,
        tipClass: 'infobox',
        offset: [-130,5],
        onBeforeShow: function() {
            var theUrl = 'https://www.mijntuin.org/ajax/info/' + type + '/' + id;
            var $tip = this.getTip();
            $tip.text("loading...");
            $.ajax({
                url: theUrl,
                success: function(response) {
                    $tip.html(response);
                }
            });
        }
    });
}
function hasHtml5Validation () {
    return typeof document.createElement('input').checkValidity === 'function';
}
function stripHtmlFromText(html)
{
    var tmp = document.implementation.createHTMLDocument("New").body;
    tmp.innerHTML = html;
    return tmp.textContent || tmp.innerText || "";
}

$(function(){

    $('body').on('click', "a[target='_blank']", function(e){
        e.preventDefault();
        var href = $(this).attr('href');
        if(href){
            window.opener = null;
            var win = window.open(href, '_blank');
            win.focus();
        }
    });


    jQuery.each(jQuery('textarea[data-autoresize]'), function() {
        var offset = this.offsetHeight - this.clientHeight;

        var resizeTextarea = function(el) {
            jQuery(el).css('height', 'auto').css('height', el.scrollHeight + offset);
        };
        jQuery(this).on('keyup input', function() { resizeTextarea(this); }).removeAttr('data-autoresize');
    });

    if(jQuery().owlCarousel) {
        $(".owl-carousel").owlCarousel({margin: 5, items: 4, autowidth: false});
    }
    if(jQuery().masonry && jQuery().imagesLoaded) {
        $('.masonry').imagesLoaded( function() {
            $(".masonry").masonry({
                itemSelector: 'a',
                columnWidth: 250
            });
        });
    }



    $('a.more').prepend("<i></i>");

    var $notice = $('.notice.hide-delay');
    if($notice){
        $notice.show();
        setTimeout(function(){ $notice.not('#attentionBox,#signUpBox').slideUp(700)}, 4000);
    }

    if (hasHtml5Validation()) {
        $('.validate-form').submit(function (e) {
            if (!this.checkValidity()) {
                e.preventDefault();
                $(this).addClass('invalid');
            } else {
                $(this).removeClass('invalid');
            }
        });
    }

    $(".tooltip").tipsy({html: true, gravity: 'n', live: true});
        $("ul.right li").hover(
        function(){ $(this).addClass("hover"); },
        function(){ $(this).removeClass("hover"); }
    );

    if(!Modernizr.input.placeholder){
        $('[placeholder]').focus(function() {
            var input = $(this);
            if (input.val() == input.attr('placeholder')) {
                input.val('');
                input.removeClass('placeholder');
            }
        }).blur(function() {
                var input = $(this);
                if (input.val() == '' || input.val() == input.attr('placeholder')) {
                    input.addClass('placeholder');
                    input.val(input.attr('placeholder'));
                }
            }).blur();
        $('[placeholder]').parents('form').submit(function() {
            $(this).find('[placeholder]').each(function() {
                var input = $(this);
                if (input.val() == input.attr('placeholder')) {
                    input.val('');
                }
            })
        });

    }

    $('.avatar, .plant, .user').each(function() {
        //enableTooltip(this);
    });
    $('a.confirm').click(function(){
        return confirm("Bent u zeker?");
    });
    $('.clickable').click(function(){
       var href = $(this).find('a').attr("href");
        if(href){
            window.location.href = href;
        }
    });


    $('.dropdown-toggle').click(function() {
        var dropdownList = $(this).parent().find('.dropdown-menu');
        var dropdownOffset = $(this).offset();
        var offsetLeft = dropdownOffset.left;
        var dropdownWidth = dropdownList.width();
        var docWidth = $(window).width();

        var subDropdown = $(this).parent().find('.dropdown-menu').eq(1);
        var subDropdownWidth = subDropdown.width();

        var isDropdownVisible = (offsetLeft + dropdownWidth <= docWidth);
        var isSubDropdownVisible = (offsetLeft + dropdownWidth + subDropdownWidth <= docWidth);

        if (!isDropdownVisible || !isSubDropdownVisible) {
            $('.dropdown-menu').addClass('pull-right');
        } else {
            $('.dropdown-menu').removeClass('pull-right');
        }
    });

    if($('#numberOfNotificationsNotice')){

        $('#closeNumberOfNotificationsNotice').on('click', function(){
            Cookies.set('closeNumberOfNotificationsNotice', '1', { expires: 30 });
            $('#numberOfNotificationsNotice').hide();
        });
    }
});
</script>
