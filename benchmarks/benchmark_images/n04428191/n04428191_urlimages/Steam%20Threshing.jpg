        function getCookie_weather_widge(cname,return_if_fail)
        {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for(var i=0; i<ca.length; i++)
            {
                var c = ca[i];
                while (c.charAt(0)==' ') c = c.substring(1);
                if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
            }
            return (return_if_fail);
        }
            function generate_widge_html_ajax()
                {

                    var lat=getCookie_weather_widge("weather_set_latitude","51.5072");
                    var long=getCookie_weather_widge("weather_set_longitude","0.1275");
                      lat=parseFloat(lat);
                      lat=(lat+180);
                      lat=lat.toFixed(2);
                      lat=parseFloat(lat)*100;
                      long=parseFloat(long)+180;
                      long=long.toFixed(2);
                      long=parseFloat(long)*100;
                      var cache_no=lat.toString()+long.toString();
                    $.ajax
                    (
                    {
                        type: "POST",
                        url:"/weatherajax_"+cache_no ,
