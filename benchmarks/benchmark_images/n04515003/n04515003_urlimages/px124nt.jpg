  $(function(){

    var modalWindow = '#wow-modal-window-';
    var modalClose = '#wow-modal-close-';
    var modalOpen = '#wow-modal-id-';

    var url = $(modalWindow + '1 iframe').attr('src');
    var url2 = $(modalWindow + '2 iframe').attr('src');
    var url3 = $(modalWindow + '3 iframe').attr('src');
    

    $(modalOpen + '1 a').on('click', function() {
      $(modalWindow + '1').show();
      $(modalWindow + '1 iframe').attr('src', url);
    });

    $(modalClose + '1').on('click', function() {
      $(modalWindow + '1').hide();
      $(modalWindow + '1 iframe').attr('src', '');
    });

    $(modalOpen + '2 a').on('click', function() {
      $(modalWindow + '2').show();
      $(modalWindow + '2 iframe').attr('src', url2);
    });

    $(modalClose + '2').on('click', function() {
      $(modalWindow + '2').hide();
      $(modalWindow + '2 iframe').attr('src', '');
    });

    $(modalOpen + '3 a').on('click', function() {
      $(modalWindow + '3').show();
      $(modalWindow + '3 iframe').attr('src', url3);
    });

    $(modalClose + '3').on('click', function() {
      $(modalWindow + '3').hide();
      $(modalWindow + '3 iframe').attr('src', '');
    });
    

  });

});
</script>
  
  
