<SCRIPT type="text/javascript">
window.vid = -1;
function loadNews(id) {
    if( typeof window.data[id] != "undefined" ) {
        if( window.last_section ) document.getElementById('h'+window.last_section).className = '';

        window.last_section = id;
        document.getElementById('h'+id).className = 'on';

        var o = document.getElementById('section_sel');
        for(var i = 0; i < o.options.length; i++) {
            if( o.options[i].value == id ) {
                o.selectedIndex = i;
                break;
            }
        }

        var news = "";
        for(var i in window.data[id]) {
            news += '<div class="news_item"><div class="news_pub">'+window.data[id][i][0]+'</div><h1><a href="'+window.data[id][i][3]+'" target="_blank">'+window.data[id][i][1]+'</a></h1>'+window.data[id][i][2]+'</div>';
        }
        document.getElementById('news').innerHTML = news;
        document.getElementById('video').style.display = ((id == 'top')?'':'none');
        document.getElementById('video').style.display = ((id == 'top')?'':'none');
        document.getElementById('video_next').style.display = ((id == 'top')?'':'none');
    }

    return false;
}
function showNextVideo() {
    window.vid++;
    if( window.vid >= window.data['vids'].length ) window.vid = 0;
    document.getElementById('video').innerHTML = '<iframe src="http://www.youtube.com/embed/'+window.data['vids'][window.vid][2]+'?autoplay=1" frameborder="0"/>';
    return false;
}
</SCRIPT>
