    <script>
        var resizeThrottle = false;

        $('document').ready(function(){
            var mySwiper = new Swiper('.swiper-container', {
                loop:true,
                slidesPerView:1,
                centeredSlides: true,
                autoplay:4000,
                pagination:'.swiper-pagination',
                paginationClickable: true
            });

            //Init max height var
            var maxHeight;
            var curHeight;

            //Set height of all contentSections to be equal on load and window resize

            setTimeout(resizeSections,500);

            $(window).resize(function(){

                if(resizeThrottle == true) { return; }

                //Set the throttle to true
                resizeThrottle = true;
                setTimeout(function(){
                    resizeThrottle = false;
                    resizeSections();
                }, 750);

            });
        });

        function resizeSections()
        {

            return;

            $('.contentSectionContainer').each(function(){

                //Reset for each new section
                maxHeight = 0;

                //Get the height of each content section in order to set the max height
                sections = $(this).find('.contentSection');

                for(a=0; a<sections.length; a++)
                {
                    curHeight = $(sections[a]).outerHeight();

                    console.log('Cur Height: ' + curHeight);

                    //Set max height
                    if(curHeight > maxHeight) { maxHeight = curHeight; }
                };

                //Loop through each content section and set the height as well as the child elements height
                for(a=0; a<sections.length; a++)
                {

                    if($(sections[a]).find('section').length == 1)
                    {
                        $(sections[a]).children().each(function(){

                            $(this).css('height', maxHeight);

                        });
                    }
                }
            });
        }

    </script>

